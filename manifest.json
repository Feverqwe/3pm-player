{
    "manifest_version": 2,
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "default_locale": "en",
    "version": "0.6.78",
    "minimum_chrome_version": "23",
    "icons": {
        "16": "icon_16.png",
        "128": "icon_128.png"
    },
    "app": {
        "background": {
            "scripts": ["scripts/background.js"]
        }
    },
    "offline_enabled": true,
    "permissions": ["app.runtime", "storage", "contextMenus", {"socket": ["tcp-listen:*:*"]}, {"fileSystem": ["retainEntries", "directory"]}, "fullscreen", "identity", "notifications", "app.window", "https://api.vk.com/*", "https://*.box.com/*", "https://dl.boxcloud.com/*", "https://*.livefilestore.com/*", "http://api.soundcloud.com/*"]
}
