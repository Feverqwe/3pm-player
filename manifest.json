{
    "manifest_version": 2,
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "default_locale": "en",
    "version": "0.7.85",
    "minimum_chrome_version": "23",
    "icons": {
        "16": "icon_16.png",
        "128": "icon_128.png"
    },
    "app": {
        "background": {
            "scripts": ["scripts/background.js"]
        }
    },
    "offline_enabled": true,
    "permissions": [
        {"mediaGalleries": ["read"]},
        "app.runtime", "storage",
        "contextMenus", "fullscreen",
        "identity", "notifications",
        "app.window","power",
        {"socket": ["tcp-listen:*:*"]},
        {"fileSystem": ["retainEntries", "directory"]},
        "https://api.vk.com/*",
        "http://*.vk.me/*",
        "https://*.box.com/*",
        "https://dl.boxcloud.com/*",
        "https://*.livefilestore.com/*",
        "https://skydrive.live.com/",
        "http://api.soundcloud.com/*",
        "http://userserve-ak.last.fm/*",
        "http://cdn.last.fm/*",
        "http://chart.apis.google.com/*",
        "http://images.amazon.com/*",
        "https://api-web.soundcloud.com/explore/*"
    ]
}
